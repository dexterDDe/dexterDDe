/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/app/*"]
    }
  },
  "exclude": ["node_modules", "e2e"]
}
